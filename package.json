	"scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js' --only-mapped",
		"start": "craco start",
		"build": "craco build",
		"test": "craco test --coverage --watchAll=false --reporters=default --reporters=jest-junit",
		"test:local": "craco test",
		"test:local:file": "craco test",
		"env": "node env",
		"lint": "eslint . --ignore-path .gitignore",
		"prettier": "prettier --write . --ignore-path .gitignore",
		"prettier:check": "prettier --check . --ignore-path .gitignore",
		"typescript:check": "tsc --noEmit",
		"link:ui": "yarn link @sergiogc9/react-ui @sergiogc9/react-ui-collections @sergiogc9/react-ui-theme",
		"unlink:ui": "yarn unlink @sergiogc9/react-ui @sergiogc9/react-ui-collections @sergiogc9/react-ui-theme && yarn install --force",
		"link:utils": "yarn link @sergiogc9/react @sergiogc9/react-utils @sergiogc9/react-hooks",
		"unlink:utils": "yarn unlink @sergiogc9/react-ui @sergiogc9/react-ui-theme && yarn install --force",
		"update-lang": "node src/scripts/updateLocales.js && prettier --write ./src/i18n/locales"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn prettier:check && yarn lint && yarn typescript:check && yarn test",
			"pre-push": "foxcli is-branch-synced"
		}
	},
	"engines": {
		"node": ">=14.x.x",
		"npm": ">=6.0.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}

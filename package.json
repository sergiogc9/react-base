{
	"name": "react-base",
	"version": "0.1.0",
	"private": true,
	"devDependencies": {
		"@types/enzyme": "3.10.3",
		"@types/enzyme-adapter-react-16": "1.0.5",
		"@types/jest": "24.0.23",
		"@types/lodash": "4.14.149",
		"@types/mockdate": "2.0.0",
		"@types/moment-timezone": "0.5.12",
		"@types/node": "12.12.14",
		"@types/papaparse": "5.0.2",
		"@types/react": "16.9.13",
		"@types/react-dom": "16.9.4",
		"@types/react-redux": "7.1.5",
		"@types/react-router-dom": "5.1.3",
		"@types/react-router-redux": "5.0.18",
		"@types/react-transition-group": "4.2.3",
		"@types/redux-logger": "3.0.7",
		"@types/redux-mock-store": "1.0.1",
		"@types/validator": "12.0.1",
		"@types/webfontloader": "1.6.29",
		"@types/xregexp": "3.0.30",
		"babel-jest": "24.9.0",
		"enzyme": "3.10.0",
		"enzyme-adapter-react-16": "1.15.1",
		"enzyme-to-json": "3.4.3",
		"jest": "24.9.0",
		"jest-cli": "24.9.0",
		"jest-mock-axios": "3.1.2",
		"mockdate": "2.0.5",
		"papaparse": "5.1.0",
		"react-router-test-context": "0.1.0",
		"redux-mock-store": "1.5.3",
		"redux-saga-test-plan": "4.0.0-rc.3",
		"source-map-explorer": "2.1.1",
		"ts-jest": "24.2.0",
		"ts-node": "8.5.2",
		"tslint": "5.20.1"
	},
	"dependencies": {
		"axios": "0.19.0",
		"connected-react-router": "6.6.1",
		"history": "4.10.1",
		"i18next": "19.0.1",
		"lodash": "4.17.15",
		"lucene": "2.1.1",
		"moment": "2.24.0",
		"moment-timezone": "0.5.27",
		"node-sass": "4.13.0",
		"query-string": "6.9.0",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-facebook": "8.1.4",
		"react-ga": "2.7.0",
		"react-i18next": "11.2.3",
		"react-md": "1.12.3",
		"react-redux": "7.1.3",
		"react-router-dom": "5.1.2",
		"react-scripts": "3.2.0",
		"react-transition-group": "4.3.0",
		"redux": "4.0.4",
		"redux-devtools-extension": "2.13.8",
		"redux-saga": "1.1.3",
		"typescript": "3.7.2",
		"typescript-fsa": "3.0.0",
		"validator": "12.1.0",
		"webfontloader": "1.6.28",
		"xregexp": "4.2.4"
	},
	"scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"start": "EXTEND_ESLINT=true react-scripts start",
		"build": "EXTEND_ESLINT=true react-scripts build",
		"test": "react-scripts test --coverage --watchAll=false __tests__/suites",
		"test:local": "react-scripts test --coverage __tests__/suites",
		"test:local:file": "react-scripts test",
		"env": "node env",
		"postinstall": "ln -sfn ../src node_modules/@src"
	},
	"engines": {
		"node": "12.13.1",
		"npm": ">=6.12.1"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"jest": {
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"coverageReporters": [
			"json",
			"lcov",
			"text",
			"clover",
			"text-summary"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx}",
			"!**/index.*",
			"!src/Main.tsx",
			"!src/components/Newsletter/**/*",
			"!src/store/newsletter/**/*",
			"!src/components/App/Facebook/**/*",
			"!src/types/**",
			"!src/scripts/**",
			"!src/lib/luceneValidator.ts"
		],
		"coverageThreshold": {
			"global": {
				"statements": 0,
				"branches": 0,
				"functions": 0,
				"lines": 0
			}
		}
	}
}

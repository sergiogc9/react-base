		"xregexp": "4.2.4"
	},
	"scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"start": "EXTEND_ESLINT=true react-scripts start",
		"build": "EXTEND_ESLINT=true react-scripts build",
		"test": "react-scripts test --coverage --watchAll=false __tests__/suites",
		"test:local": "react-scripts test --coverage __tests__/suites",
		"test:local:file": "react-scripts test",
		"test:puppeteer": "node src/__puppeteer__/start",
		"test:puppeteer:debug": "env DEBUG=* env DEBUG_COLORS=true node src/__puppeteer__/start",
		"env": "node env",
		"postinstall": "ln -sfn ../src node_modules/@src",
		"update-lang": "ts-node -O '{ \"module\": \"commonjs\" }' ./src/scripts/multilanguageDiscover.ts",
		"brotli": "ts-node -O '{ \"module\": \"commonjs\" }' ./src/scripts/brotliCompression.ts"
	},
	"engines": {
		"node": "10.16.x",
		"npm": ">=5.10.0"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"jest": {
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"coverageReporters": [
			"json",
			"lcov",
			"text",
			"clover",
			"text-summary"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx}",
			"!**/index.*",
			"!src/Main.tsx",
			"!src/components/Newsletter/**/*",
			"!src/store/newsletter/**/*",
			"!src/components/App/Facebook/**/*",
			"!src/types/**",
			"!src/scripts/**",
			"!src/__puppeteer__/**",
			"!src/lib/luceneValidator.ts"
		],
		"coverageThreshold": {
			"global": {
				"statements": 96,
				"branches": 93.93,
				"functions": 96,
				"lines": 96
			}
		}
	}
}
